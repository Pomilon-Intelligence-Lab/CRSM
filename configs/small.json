        "grad_accum": 1,
        "use_amp": false
    },
    "dynamics": {
        "dynamics_samples": 1000,
        "dynamics_epochs": 10,
        "dynamics_lr": 1e-3
    },
    "data": {
        "data_dir": "data/dummy_data",
        "traces_path": "data/dummy_traces.jsonl"
    },
    "tokenizer": null,
